{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "extension": [
        ".ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "**/*.d.ts",
        "**/node_modules/*"
    ],
    "reporter": "lcov",
    "report-dir": "./coverage",
    "temp-dir": "./coverage/.nyc_output"
}

